<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient_background"
    tools:context=".MainDisplay">

    <Button
        android:id="@+id/setPassVisibility"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="49dp"
        android:layout_marginBottom="2dp"
        android:fontFamily="@font/montserrat_medium"
        android:text="@string/show"
        app:layout_constraintBottom_toTopOf="@+id/pw"
        app:layout_constraintEnd_toEndOf="@+id/pw"
        tools:ignore="HardcodedText" />

    <Spinner
        android:id="@+id/spinner2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="11dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/pw"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@null"
        android:backgroundTint="#FFFFFF"
        android:fontFamily="@font/montserrat_semibold"
        android:textColor="#FFFFFF"
        android:password="true"
        android:textAlignment="textStart"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        tools:ignore="Deprecated,TextViewEdits" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginBottom="21dp"
        android:fontFamily="@font/roboto_mediumitalic"
        android:text="@string/username"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/un"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/un"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="148dp"
        android:textAlignment="textStart"
        android:textSize="30sp"
        android:textColor="#FFFFFF"
        android:fontFamily="@font/montserrat_semibold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginTop="5dp"
        android:fontFamily="@font/roboto_mediumitalic"
        android:textColor="#FFFFFF"
        android:text="@string/password"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/un" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="14dp"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/roboto_mediumitalic"
        android:textColor="#FFFFFF"
        android:text="@string/usage"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pw" />

    <TextView
        android:id="@+id/us"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:textAlignment="textStart"
        android:fontFamily="@font/montserrat_semibold"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/labelChg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="55dp"
        android:fontFamily="@font/roboto_mediumitalic"
        android:textColor="#FFFFFF"
        android:text="@string/change_password"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/changeLogin"
        app:layout_constraintTop_toTopOf="@+id/changeLogin" />

    <TextView
        android:id="@+id/labelAdd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="55dp"
        android:labelFor="@id/addAccount"
        android:fontFamily="@font/roboto_mediumitalic"
        android:textColor="#FFFFFF"
        android:text="@string/add_account"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/addAccount"
        app:layout_constraintTop_toTopOf="@+id/addAccount" />

    <TextView
        android:id="@+id/labelUpd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="11dp"
        android:labelFor="@id/updateAccount"
        android:fontFamily="@font/roboto_mediumitalic"
        android:textColor="#FFFFFF"
        android:text="@string/update_account"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/updateAccount"
        app:layout_constraintTop_toTopOf="@+id/updateAccount" />

    <TextView
        android:id="@+id/labelDel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="15dp"
        android:labelFor="@id/deleteAccount"
        android:fontFamily="@font/roboto_mediumitalic"
        android:textColor="#FFFFFF"
        android:text="@string/delete_account"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/labelChg"
        app:layout_constraintStart_toStartOf="@+id/labelUpd"
        app:layout_constraintTop_toTopOf="@+id/deleteAccount" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/updateAccount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:backgroundTint="#FFFFFF"
        app:elevation="0dp"
        app:layout_constraintStart_toStartOf="@+id/deleteAccount"
        app:layout_constraintTop_toBottomOf="@+id/deleteAccount"
        app:srcCompat="@drawable/update"
        tools:ignore="ContentDescription" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/addAccount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:backgroundTint="#FFFFFF"
        app:elevation="0dp"
        app:fabSize="normal"
        app:layout_constraintStart_toStartOf="@+id/updateAccount"
        app:layout_constraintTop_toBottomOf="@+id/updateAccount"
        app:srcCompat="@drawable/add"
        tools:ignore="ContentDescription" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/deleteAccount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:backgroundTint="#FFFFFF"
        app:elevation="0dp"
        app:layout_constraintStart_toStartOf="@+id/changeLogin"
        app:layout_constraintTop_toBottomOf="@+id/changeLogin"
        app:srcCompat="@drawable/delete"
        tools:ignore="ContentDescription" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/changeLogin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="244dp"
        android:clickable="true"
        app:backgroundTint="#FFFFFF"
        app:elevation="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/change_password"
        tools:ignore="ContentDescription" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/userAction"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="20dp"
        android:clickable="true"
        app:backgroundTint="#FFFFFF"
        app:elevation="0dp"
        app:fabSize="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/user"
        tools:ignore="ContentDescription" />


</androidx.constraintlayout.widget.ConstraintLayout>
